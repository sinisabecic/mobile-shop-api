{
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "window.zoomLevel": 0,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "explorer.confirmDelete": false,
    "editor.formatOnPaste": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.minimap.enabled": true,
    "breadcrumbs.enabled": false,
    "workbench.colorTheme": "GitHub Dark",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true
    },
    "java.home": "C:\\Program Files\\Java\\java-1.8.0-openjdk-1.8.0.242-3.b08.redhat.windows.x86_64",
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml",
        "editor.formatOnSave": true
    },
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify",
        "editor.formatOnSave": true
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "editor.codeActionsOnSave": {
        // For ESLint
        "source.fixAll.eslint": false
    },
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.confirmDragAndDrop": false,
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[sass]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    // U slucaju da ne radi, poci na adresu extensions i azurirati naziv foldera u "php-cs-fixer.executablePath"
    "php-cs-fixer.executablePath": "C:\\Users\\User\\.vscode\\extensions\\junstyle.php-cs-fixer-0.1.155\\php-cs-fixer.phar",
    // "php-cs-fixer.executablePathWindows": "C:\\Users\\User\\.vscode\\extensions\\php-cs-fixer-v2.bat", //eg: php-cs-fixer.bat
    "php-cs-fixer.onsave": true,
    "php-cs-fixer.rules": "@PSR2",
    "php-cs-fixer.config": ".php_cs;.php_cs.dist",
    "php-cs-fixer.allowRisky": false,
    "php-cs-fixer.pathMode": "override",
    "php-cs-fixer.exclude": [],
    "php-cs-fixer.autoFixByBracket": true,
    "php-cs-fixer.autoFixBySemicolon": false,
    "php-cs-fixer.formatHtml": true,
    "php-cs-fixer.documentFormattingProvider": true,
    "[php]": {
        "editor.defaultFormatter": "kokororin.vscode-phpfmt",
        "editor.formatOnSave": true
        // "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
    },
    "files.associations": {
        "*.php": "php"
    },
    "[blade]": {
        "editor.defaultFormatter": "junstyle.php-cs-fixer",
        "editor.formatOnSave": true
    },
}